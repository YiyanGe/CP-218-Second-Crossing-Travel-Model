      TOLLMD_S2  = 180.20 / 251.99 * MIN(300, MAX(0,(600 - 400)))
      TOLLMD_S3  = 180.20 / 251.99 * MIN(300, MAX(0,(600 - 400)))
      TOLLMD_VSM = 180.20 / 251.99 * MIN(300, MAX(0,(600 - 400)))
