      TOLLEA_SML = 180.20 / 251.99 * MIN(300, MAX(0,(600 - 400)))
      TOLLEA_MED = 180.20 / 251.99 * MIN(300, MAX(0,(600 - 1875)))
      TOLLEA_LRG = 180.20 / 251.99 * MIN(300, MAX(0,(600 - 3437.5)))
      
